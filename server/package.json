{
  "name": "hex-online-server",
  "version": "1.0.0",
  "main": "src/main.ts",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' -r tsconfig-paths/register src/main.ts",
    "buildprotobuf": "pbjs --es6 -t static-module -w es6 -o src/protobuf/bundle.js src/protobuf/awesome.proto && pbts -o src/protobuf/bundle.d.ts src/protobuf/bundle.js"
  },
  "devDependencies": {
    "nodemon": "^2.0.9",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "express": "^4.17.1",
    "socket.io": "^4.1.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.5"
  }
}
